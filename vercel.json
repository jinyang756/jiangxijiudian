{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://154.221.19.68:8443/api/$1"
    },
    {
      "src": "/_/(.*)",
      "dest": "https://154.221.19.68:8443/_/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite"
}